{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "annotations": {
      "type": "string"
    },
    "toolSpecificSample": {
      "type": "object",
      "properties": {
        "sampleProperty": {
          "type": "string"
        }
      }
    },
    "sources": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string"
          },
          "href": {
            "type": "string"
          },
          "validHref": {
            "type": "boolean"
          }
        }
      }
    }
  },
  "required": [
    "title"
  ]
}
